<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Suite" parallel="tests">
	<listeners>
		<listener class-name="com.obsqura.maven.testScripts.Listeners"/>
	</listeners>
	<parameter name="browser" value="chrome"></parameter>
	<parameter name="username" value="admin"></parameter>
	<parameter name="password" value="admin"></parameter>
    <test thread-count="5" name="Test">
 		<classes>
			<class name="com.obsqura.maven.testScripts.LoginTest">
			 <methods>
				  <include name="verify_login"></include>
				  <include name="verify_LoginDataFromExcel"></include>
			 </methods>
			 </class>
			 <class name="com.obsqura.maven.testScripts.ManagepageTest">
			 <methods>
				 <include name="verify_newButton"></include>
				 <include name="verify_searchButton"></include>
			 </methods>
			 </class>
 		</classes>
  	</test> <!-- Test -->
</suite> <!-- Suite -->
